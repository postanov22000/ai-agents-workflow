{
  "cleanUrls": true,
  "rewrites": [
   
    {
      "source": "/signin",
      "destination": "https://replyzeai.onrender.com/signin"
    },
    {
      "source": "/signin/:rest*",      
      "destination": "https://replyzeai.onrender.com/signin"
    },
    {
      "source": "/connect-smtp",
      "destination": "https://replyzeai.onrender.com/connect-smtp"
    },
    {
      "source": "/connect_gmail",
      "destination": "https://replyzeai.onrender.com/connect_gmail"
    },


    {
      "source": "/",
      "destination": "/PUBLIC/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/PUBLIC/$1.html"
    }
  ]
}
